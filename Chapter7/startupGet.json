[{
  "description": "模拟一个没有参数的get请求",
  "request": {
    "uri": "/getdemo",
    "method": "get"
  },
  "response": {
    "text": "这是一个没有入参的get请求",
    "headers": {
      "Content-Type": "text/html;charset=gbk"
